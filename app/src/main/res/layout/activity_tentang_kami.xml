    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <RelativeLayout
            android:id="@+id/rl_tentang2"
            android:layout_width="match_parent"
            android:layout_height="110dp"
            android:background="@drawable/button" >

            <ImageView
                android:id="@+id/iv_tentangkami"
                android:layout_width="112dp"
                android:layout_height="82dp"
                android:layout_centerHorizontal="true"
                android:foregroundGravity="center"
                android:src="@drawable/ic_help_outline_black_24dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/iv_tentangkami"
                android:layout_centerHorizontal="true"
                android:text="Tentang Kami"
                android:textSize="18dp" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@drawable/button"
            android:layout_below="@id/rl_tentang2"
            android:layout_marginTop="20dp" >

            <TextView
                android:id="@+id/tv_teamkami"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Team Kami"
                android:textSize="16dp"
                android:drawableLeft="@drawable/ic_group_black_24dp"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="5dp"/>

            <de.hdodenhof.circleimageview.CircleImageView
                android:id="@+id/img_iqbal"
                android:layout_width="80dp"
                android:layout_height="80dp"
                android:src="@drawable/boy"
                android:layout_below="@id/tv_teamkami"
                android:layout_marginTop="10dp"
                android:layout_marginLeft="5dp"/>

            <de.hdodenhof.circleimageview.CircleImageView
                android:id="@+id/img_safira"
                android:layout_width="80dp"
                android:layout_height="80dp"
                android:src="@drawable/boy"
                android:layout_below="@id/img_iqbal"
                android:layout_marginTop="5dp"
                android:layout_marginLeft="5dp"/>

            <de.hdodenhof.circleimageview.CircleImageView
                android:id="@+id/img_dhifan"
                android:layout_width="80dp"
                android:layout_height="80dp"
                android:src="@drawable/boy"
                android:layout_below="@id/img_safira"
                android:layout_marginTop="5dp"
                android:layout_marginLeft="5dp"/>

            <de.hdodenhof.circleimageview.CircleImageView
                android:id="@+id/img_sulthon"
                android:layout_width="80dp"
                android:layout_height="80dp"
                android:src="@drawable/boy"
                android:layout_below="@id/img_dhifan"
                android:layout_marginTop="5dp"
                android:layout_marginLeft="5dp"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/tv_teamkami"
                android:layout_centerInParent="true"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="25dp"
                android:layout_toRightOf="@id/img_iqbal"
                android:text="Rizky Maulana Iqbal\n175150201111047"
                android:textSize="20dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/tv_teamkami"
                android:layout_centerInParent="true"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="110dp"
                android:layout_toRightOf="@id/img_safira"
                android:text="Safira Dyah Karina\n175150200111141"
                android:textSize="20dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/tv_teamkami"
                android:layout_centerInParent="true"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="195dp"
                android:layout_toRightOf="@id/img_dhifan"
                android:text="Dhifan Diandra Heriswan\n175150207111013"
                android:textSize="20dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/tv_teamkami"
                android:layout_centerInParent="true"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="280dp"
                android:layout_toRightOf="@id/img_sulthon"
                android:text="Sulthon Akhdan Ghali\n175150200111065"
                android:textSize="20dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Kelompok 6\nSistem Pakar-B"
                android:textSize="20dp"
                android:layout_below="@id/img_sulthon"
                android:layout_marginTop="10dp"
                android:drawableLeft="@drawable/ic_domain_black_24dp"/>

        </RelativeLayout>

    </RelativeLayout>

</androidx.constraintlayout.widget.ConstraintLayout>